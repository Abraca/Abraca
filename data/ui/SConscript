Import('env')

lst = Split("""
    about.xml
    coverart.xml
    main_menu.xml
    mediainfo.xml
    server_browser.xml
""")

ui, header, vapi = env.BinaryBlob('abraca_ui', lst)

env.Append(CPPPATH = [Dir('.')])
env.Append(VALAPKGPATH = [Dir('.')])
env.Append(VALAPKGS = ['abraca_ui'])

Export('ui')
