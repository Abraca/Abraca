<?xml version="1.0" encoding="utf-8"?>
<glade-catalog supports="gtkbuilder" name="abraca" library="gladeabraca" depends="gtk+">
	<init-function>glade_abraca_init</init-function>
	<glade-widget-classes>
		<!-- Toplevels -->
		<glade-widget-class name="AbracaMedialibInfoDialog" generic-name="mediainfodialog" title="Medialib Information Dialog"/>

		<!-- Widgets -->
		<glade-widget-class name="AbracaRatingEntry" generic-name="ratingentry" title="Rating Entry"/>

		<!-- Cell Renderers -->
		<glade-widget-class name="AbracaCellRendererCollection" generic-name="cellrenderercollection" title="Collection Cell Renderer">
			<properties>
				<property save="False" id="pixbuf" custom-layout="True"/>
				<property save="False" id="attr-pixbuf" default="-1" custom-layout="True" name="Pixbuf column">
					<parameter-spec>
						<type>GParamInt</type>
						<min>-1</min>
					</parameter-spec>
					<tooltip>The column in the model to load the value from</tooltip>
				</property>
				<property visible="False" save="False" id="use-attr-pixbuf" default="True">
					<parameter-spec>
						<type>GParamBoolean</type>
					</parameter-spec>
				</property>
			</properties>
		</glade-widget-class>

		<glade-widget-class name="AbracaCellRendererTogglePixbuf" generic-name="cellrenderertogglepixbuf" title="Toggle Pixbuf Renderer">
			<properties>
				<property save="False" id="activatable" custom-layout="True"/>
				<property save="False" id="attr-activatable" default="-1" custom-layout="True" name="Active column">
					<parameter-spec>
						<type>GParamInt</type>
						<min>-1</min>
					</parameter-spec>
					<tooltip>The column in the model to load the value from</tooltip>
				</property>
				<property visible="False" save="False" id="use-attr-activatable" default="True">
					<parameter-spec>
						<type>GParamBoolean</type>
					</parameter-spec>
				</property>

				<property save="False" id="active" custom-layout="True"/>
				<property save="False" id="attr-active" default="-1" custom-layout="True" name="Active column">
					<parameter-spec>
						<type>GParamInt</type>
						<min>-1</min>
					</parameter-spec>
					<tooltip>The column in the model to load the value from</tooltip>
				</property>
				<property visible="False" save="False" id="use-attr-active" default="True">
					<parameter-spec>
						<type>GParamBoolean</type>
					</parameter-spec>
				</property>
			</properties>
		</glade-widget-class>
	</glade-widget-classes>
	<glade-widget-group name="abraca" title="Abraca">
		<glade-widget-class-ref name="AbracaMedialibInfoDialog"/>
		<glade-widget-class-ref name="AbracaRatingEntry"/>
	</glade-widget-group>
</glade-catalog>
